{
    "extends": [
        "next/core-web-vitals",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": ["react"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/no-unknown-property": ["error", { "ignore": ["css"] }],
        "react/react-in-jsx-scope": "off"
    }
}
